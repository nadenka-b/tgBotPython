# КФУ Bot - Файл .env
# .env - Переменные окружения (ЗАПОЛНИТЬ ВРУЧНУЮ)

# ========== TELEGRAM BOT ==========
# Получить токен от @BotFather в Telegram
BOT_TOKEN=YOUR_BOT_TOKEN_HERE

# ID администраторов (через запятую без пробелов)
ADMIN_IDS=123456789,987654321

# ========== PARSER SETTINGS ==========
# URL сайта КФУ
KFU_BASE_URL=https://abiturient.kpfu.ru/entrant/abit_entrant_originals_list

# Timeout для запросов (в секундах)
PARSER_TIMEOUT=30

# Максимальное количество повторов при ошибке
PARSER_MAX_RETRIES=3

# ========== OPTIONAL ==========
# Режим отладки (True/False)
DEBUG=False
